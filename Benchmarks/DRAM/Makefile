all: Simple *op

Simple:
	make -C Simple

1op:
	make -C 1op

2op:
	make -C 2op

3op:
	make -C 3op

4op:
	make -C 4op

5op:
	make -C 5op

clean: 
	make -C Simple clean
	make -C 1op clean
	make -C 2op clean
	make -C 3op clean
	make -C 4op clean
	make -C 5op clean

.PHONY: all Simple *op clean